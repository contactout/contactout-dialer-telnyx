# Telnyx WebRTC Configuration
# Copy this file to .env.local and fill in your actual Telnyx credentials

# Your Telnyx API Key (from Telnyx Portal > API Keys)
NEXT_PUBLIC_TELNYX_API_KEY=your_telnyx_api_key_here

# Your SIP Username (from Telnyx Portal > SIP > Credentials)
NEXT_PUBLIC_TELNYX_SIP_USERNAME=your_sip_username_here

# Your SIP Password (from Telnyx Portal > SIP > Credentials)
NEXT_PUBLIC_TELNYX_SIP_PASSWORD=your_sip_password_here

# Your purchased Telnyx phone number (with country code, e.g., +1-234-567-8900)
NEXT_PUBLIC_TELNYX_PHONE_NUMBER=your_purchased_phone_number_here

# Supabase Configuration
# Get these from your Supabase project dashboard
NEXT_PUBLIC_SUPABASE_URL=your_supabase_project_url_here
NEXT_PUBLIC_SUPABASE_ANON_KEY=your_supabase_anon_key_here
